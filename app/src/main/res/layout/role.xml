<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">



    <View
        android:id="@+id/_bg__role_ek2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:background="@drawable/_bg__role_ek2_shape" />

    <TextView
        android:id="@+id/which_one_are_you_"
        android:layout_width="334dp"
        android:layout_height="59dp"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="80dp"
        android:text="@string/which_one_are_you__string"
        android:textColor="@color/welcome__color"
        android:gravity="center"
        android:textSize="30sp" />

    <TextView
        android:id="@+id/text_from_role"
        android:layout_width="match_parent"
        android:layout_height="59dp"
        android:layout_gravity="center_horizontal|center"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="134dp"
        android:gravity="center"
        android:text="@string/for_comfortable_interaction_with_the_application__specify_who_you_are_string"
        android:textColor="@color/use_the_information_on_your_device_to_log_in_to_your_account_color"
        android:textSize="18sp" />

    <RelativeLayout android:id="@+id/check_circle_family"
        android:layout_marginTop="228dp"
        android:layout_marginStart="131dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >

        <ImageView
            android:id="@+id/vector_ek178"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginStart="2dp"
            android:layout_marginTop="2dp"
            android:contentDescription="vector_ek178"
            android:src="@drawable/vector_ek178" />

        <ImageView
            android:id="@+id/vector_ek179"
            android:layout_width="10dp"
            android:layout_height="8dp"
            android:layout_marginStart="9dp"
            android:layout_marginTop="10dp"
            android:contentDescription="vector_ek179"
            android:src="@drawable/vector_ek179" />

    </RelativeLayout>

    <View
        android:id="@+id/contorno_marcado_family"
        android:layout_width="156dp"
        android:layout_height="196dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/rectangle_18_shape">

    </View>

    <Button
        android:id="@+id/family_member_button"
        android:layout_width="156dp"
        android:layout_height="196dp"
        android:layout_marginStart="15dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/rectangle_19_ek8_shape" />

    <TextView
        android:id="@+id/family_member_text"
        android:layout_width="132dp"
        android:layout_height="36dp"
        android:layout_gravity="center_horizontal|center"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="320dp"
        android:gravity="center"
        android:text="@string/family_member_string"
        android:textColor="@color/use_the_information_on_your_device_to_log_in_to_your_account_color"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/family_member_picture"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="70dp"
        android:layout_marginTop="280dp"
        android:src="@drawable/icon_family"
        android:contentDescription="icon_family" />

    <RelativeLayout android:id="@+id/check_circle_ederly"
        android:layout_marginTop="228dp"
        android:layout_marginStart="345dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >

        <ImageView
            android:src="@drawable/vector_ek178"
            android:contentDescription="vector_ek178"
            android:layout_marginStart="2dp"
            android:layout_marginTop="2dp"
            android:layout_width="25dp"
            android:layout_height="25dp" />

        <ImageView
            android:layout_width="10dp"
            android:layout_height="8dp"
            android:layout_marginStart="9dp"
            android:layout_marginTop="10dp"
            android:contentDescription="vector_ek179"
            android:src="@drawable/vector_ek179" />

    </RelativeLayout>

    <View
        android:id="@+id/contorno_marcado_ederly"
        android:layout_width="152dp"
        android:layout_height="192dp"
        android:layout_marginStart="220dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="220dp"
        android:background="@drawable/rectangle_18_shape">

    </View>


    <Button
        android:id="@+id/ederly_button"
        android:layout_width="152dp"
        android:layout_height="192dp"
        android:layout_marginStart="220dp"
        android:layout_marginLeft="230dp"
        android:layout_marginTop="220dp"
        android:layout_marginEnd="220dp"
        android:layout_marginBottom="220dp"
        android:background="@drawable/rectangle_19_ek8_shape" />


    <ImageView
        android:id="@+id/ederly_picture"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="250dp"
        android:layout_marginLeft="285dp"
        android:layout_marginTop="280dp"
        android:contentDescription="mask_group_ek1"
        android:src="@drawable/mask_group_ek1" />

    <TextView
        android:id="@+id/ederly_text"
        android:layout_width="102dp"
        android:layout_height="36dp"
        android:layout_marginLeft="255dp"
        android:layout_marginTop="320dp"
        android:gravity="center"
        android:text="@string/the_elderly_string"
        android:textColor="@color/use_the_information_on_your_device_to_log_in_to_your_account_color"
        android:textSize="18sp" />

    <Button
        android:id="@+id/continue_button"
        android:layout_width="153dp"
        android:layout_height="32dp"
        android:layout_marginLeft="120dp"
        android:layout_marginTop="600dp"
        android:background="@drawable/buttons_curts"
        android:gravity="center"
        android:text="@string/login_ek16_string"
        android:textColor="@color/white" />

</RelativeLayout>